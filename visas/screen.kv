ScreenManager:
    MenuScreen:
    ProbScreen:
    AboutScreen:
    DevsScreen:
<MenuScreen>:
    name: 'menu'
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'Images/HSMD.png'
    FloatLayout:
        orientation: 'vertical'
        MDToolbar:
            title: 'Work Visas'
            pos_hint: {"top": 1}
        Image:
            source: "Images/pass.png"
            orientation: 'vertical'
            size_hint_y: None
            size_hint: 0.4, 0.4
            pos_hint: {"center_x":0.5, "center_y":0.45}
    MDIconButton:
        icon: "account-supervisor"
        pos_hint: {"center_x": 0.89, "center_y": 0.965}
        theme_text_color: "Custom"
        text_color: 255/255, 255/255, 255/255, 1
        on_press:
            root.manager.transition.direction = "up"
            root.manager.current = 'devs'
    MDFloatingActionButton:
        icon: "information"
        pos_hint: {"center_x": 0.7, "center_y": 0.15}
        on_press:
            root.manager.transition.direction = "down"
            root.manager.current = 'about'
    MDFloatingActionButton:
        icon: "form-select"
        pos_hint: {"center_x": 0.3, "center_y": 0.15}
        on_press: 
            root.manager.current = 'prob'
            root.manager.transition.direction = "left"

<AboutScreen>:
    name: 'about'
    FloatLayout:
        MDToolbar:
            title: 'APP INFO'
            pos_hint: {"top": 1}
            theme_text_color: "Custom"
            text_color: 255/255, 255/255, 255/255, 1
        MDIconButton:
            icon: "arrow-down-bold"
            pos_hint: {"center_x": 0.89, "center_y": 0.965}
            theme_text_color: "Custom"
            text_color: 255/255, 255/255, 255/255, 1
            on_press:
                root.manager.transition.direction = "up"
                root.manager.current = 'menu'
		MDLabel:
		    markup: True
            text:
                '''APLICACIÓN MÓVIL CON APRENDIZAJE AUTOMÁTICO SUPERVISADO. LA CUAL DA UNA
                PREDICCIÓN DE SI SE LE PODRÁ OTORGAR O NO UNA VISA DE TRABAJO EN ESTADOS UNIDOS
                A UN PROFESIONISTA, EN FUNCIÓN DEL PORCENTAJE PREDECIDO.
                \nEL MODELO DE MACHINE LEARNING QUE SE USO EN ESTA APLICACIÓN FUE [b]DECISION TREES[/b]
                EL CUAL OBTUVO UNA [b]EXACTITUD[/b] DEL [b]81%[/b]. ESTE MODELO TIENE
                [b]81%[/b] DE [b]PRECISION[/b], [b]79%[/b] DE
                [b]RECALL[/b], Y [b]80%[/b] DE [b]F1 SCORE[/b].

                \n
                \n[b]**SOLAMENTE PARA FINES EDUCATIVOS**[/b]'''
            font_style: "Caption"
            halign: "center"
            theme_text_color: "Custom"
            text_color: 255/255, 255/255, 255/255, 1
            size_hint: 0.95, 0.65
            text_size: self.width, None
            height: self.texture_size[1]
            pos_hint: {'center_x':0.5, 'center_y':0.5}
            canvas.before:
                Color:
                    rgba: (71/255,113/255,156/255,0.75)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
    
<DevsScreen>:
    name: 'devs'
    FloatLayout:
        MDToolbar:
            title: '                  ABOUT US'
            pos_hint: {"top": 1}
            theme_text_color: "Custom"
            text_color: 255/255, 255/255, 255/255, 1
        Image:
            source: 'Images/FIT.png'
            pos_hint: {'center_x':0.15, 'center_y':0.965}
            size_hint_x: 0.12
            allow_stretch: True
        MDIconButton:
            icon: "arrow-up-bold"
            pos_hint: {"center_x": 0.89, "center_y": 0.965}
            theme_text_color: "Custom"
            text_color: 255/255, 255/255, 255/255, 1
            on_press:
                root.manager.transition.direction = "down"
                root.manager.current = 'menu'
    MDFloatLayout:
        MDCard:
            size_hint: (0.4, 0.5)
            pos_hint: {'center_x': 0.25, 'center_y': 0.5}
            elevation: 40
            ripple_behavior: True
            md_bg_color: app.theme_cls.primary_color
            MDBoxLayout:
                orientation: 'vertical'
                padding: '10dp'
                MDLabel:
                    markup: True
                    text: '[b]DEVELOPER:[/b]'
                    font_style: "Caption"
                    pos_hint: {'center_x':1, 'center_x':0.65}
                    color: 0, 0, 0, 1
                Image:
                    source: 'Images/Saulo.png'
                    pos_hint: {'center_x':0.5}
                    allow_stretch: True
                    size_hint: (1, 1)
                MDLabel:
                    markup: True
                    text: '[b]SAULO BOSQUEZ[/b] \n \n[ref=https://portafoliosfit.um.edu.mx/saulobosquez/]PORTAFOLIO[/ref]'
                    font_style: "Caption"
                    pos_hint: {'center_x':0.5, 'center_y':0.3}
                    color: 0, 0, 0, 1
                    on_ref_press:
                        import webbrowser
                        webbrowser.open('https://portafoliosfit.um.edu.mx/saulobosquez/')
        MDCard:
            size_hint: (0.4, 0.5)
            pos_hint: {'center_x': 0.75, 'center_y': 0.5}
            elevation: 40
            ripple_behavior: True
            md_bg_color: app.theme_cls.primary_color
            MDBoxLayout:
                orientation: 'vertical'
                padding: '10dp'
                MDLabel:
                    markup: True
                    text: '[b]DEVELOPER:[/b]'
                    font_style: "Caption"
                    pos_hint: {'center_x':1, 'center_x':0.65}
                    color: 0, 0, 0, 1
                Image:
                    source: 'Images/Kimy.png'
                    pos_hint: {'center_x':0.5}
                    allow_stretch: True
                    size_hint: (1, 1)
                MDLabel:
                    markup: True
                    text: '[b]KIMBERLY GARCIA[/b] \n \n[ref=https://portafoliosfit.um.edu.mx/kimberlygarcia/]PORTAFOLIO[/ref]'
                    font_style: "Caption"
                    pos_hint: {'center_x':0.5, 'center_y':0.3}
                    color: 0, 0, 0, 1
                    on_ref_press:
                        import webbrowser
                        webbrowser.open('https://portafoliosfit.um.edu.mx/kimberlygarcia/')
<ProbScreen>:
    name: 'prob'
    spinner: spinner
    on_enter: self.limit_spinner()
    MDBoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "       VISA PREDICTION"
            font_style: "Subtitle2"
            halign: 'left'
            pos_hint: {"top": 1}
            MDIconButton:
                icon: "arrow-left-bold"
                theme_text_color: "Custom"
                text_color: 255/255, 255/255, 255/255, 1
                pos_hint: {'center_y': 0.5}
                on_press:
                    root.manager.transition.direction = "right"
                    root.manager.current = 'menu'
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            bar_color: (71/255,113/255,156/255,0.5)
            bar_width: 12
            MDBoxLayout:
                orientation: "vertical"
                spacing: "35dp"
                padding: "12dp"
                adaptive_height: True
                pos_hint: {"top": 1}
                MDLabel:
                    text: ""
                    font_style: "Subtitle2"
                    size_hint: (1, None)
                    halign: 'center'
                    color: 0, 0, 0, 1
                MDLabel:
                    text: "JOB TITLE:"
                    font_style: "Subtitle2"
                    size_hint: (.5, 1)
                    pos_hint: {'center_x':0.5, 'center_y':0.25}
                    halign: 'center'
                    color: 0, 0, 0, 1
                MDTextFieldRect:
                    id: jobTitle
                    hint_text: 'EX. COMPUTER SYSTEMS ANALYST'
                    color: 0, 0, 0, 1
                    font_style: "Subtitle2"
                    size_hint: (0.75, None)
                    pos_hint: {'center_x':0.5, 'center_y':0.25}
                    on_text:
                MDLabel:
                    text: "FULL TIME POSITION:"
                    font_style: "Subtitle2"
                    size_hint: (1, 1)
                    pos_hint: {'center_x':0.5, 'center_y':0.25}
                    halign: 'center'
                    color: 0, 0, 0, 1
                MDFillRoundFlatButton:
                    id: ftPosition
                    size_hint: (0.75, None)
                    pos_hint: {'center_x':0.5, 'center_y':0.25}
                    text: "SELECT"
